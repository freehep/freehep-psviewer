%!PS-Adobe-2.0
%
% Type3Font.ps  red book page 341
%

8 dict begin
    /FontType 3 def
    /FontMatrix [.001 0 0 .001 0 0] def
    /FontBBox [0 0 750 750] def
    
    /Encoding 256 array def
    0 1 255 {
        Encoding exch /.notdef put
    } for
    Encoding 97 /square put
    Encoding 98 /triangle put
        
    /CharProcs 3 dict def
    CharProcs begin
        /.notdef {} def
        /square {
            0 0 moveto
            750 0 lineto
            750 750 lineto
            0 750 lineto
            closepath
            fill
        } bind def
        
        /triangle {
            0 0 moveto
            375 750 lineto
            750 0 lineto
            closepath
            fill
        } bind def
    end
        
    /BuildGlyph {
        1000 0
        0 0 750 750
        setcachedevice
        exch /CharProcs get exch
        2 copy known not {
            pop /.notdef
        } if
        get exec
    } bind def
        
    /BuildChar {
        1 index /Encoding get exch get
        1 index /BuildGlyph get exec
    } bind def
        
    currentdict
end
    
/ExampleFont exch definefont pop    
    
/ExampleFont findfont 12 scalefont setfont

36 52 moveto
100 52 lineto
stroke
36 52 moveto
(ababa) show
    